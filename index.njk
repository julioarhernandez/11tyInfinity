================================================== -->
<section id="textContent" style="background-image: url({{ home.textContent.backgroundImage }});" >
    <div class="overlay"></div>
    <div class="gradient-overlay"></div>
    <div class="row section-intro animate-this">
        <div class="col-full">
            <h3>{{home.textContent.subtitle }}</h3>
            <h1>{{home.textContent.title }}</h1>
            <p class="lead">{{ home.textContent.content }}</p>
        </div> <!-- end col-full -->
    </div> <!-- end row -->
</section> <!-- end textContent -->

<!-- Testimonials Section
================================================== -->
<section id="testimonials">

    <div class="row">
        <div class="col-twelve">
            <h2 class="animate-this">{{ home.testimonials.title }}</h2>
        </div>
    </div>

    <div class="row flex-container">

        <div id="testimonial-slider" class="flex-slider animate-this">

            <ul class="slides">
				{% for item in home.testimonials.items %}
                <li>
                    <p>
                        {{ item.content }}
                    </p>

                    <div class="testimonial-author">
                        <img src="{{ item.avatar }}" alt="Author image">
                        <div class="author-info">
							{{ item.name }}
                            <span class="position">{{ item.position }}</span>
                        </div>
                    </div>
                </li> <!-- end slide -->
				{% endfor %}
            </ul> <!-- end slides -->

        </div> <!-- end testimonial-slider -->

    </div> <!-- end flex-container -->

</section> <!-- end testimonials -->


<!-- stats
================================================== -->
<section id="clients">

    <div class="row animate-this">
        <div class="col-twelve">

            <div class="client-lists owl-carousel">
                {% for item in home.clients %}
                    <div><img src="{{ item.src }}" alt="{{ item.alt }}"></div>
                {% endfor %}
            </div>

        </div> <!-- end col-twelve -->
    </div> <!-- end row -->

</section> <!-- end clients -->


<!-- contact
================================================== -->
<section id="contact" style="background-image: url({{ home.contact.backgroundImage }});">

    <div class="overlay"></div>

    <div class="row narrow section-intro with-bottom-sep animate-this">
        <div class="col-twelve">
            <h3>{{ home.contact.subtitle }}</h3>
            <h1>{{ home.contact.title }}</h1>

            <p class="lead">{{ home.contact.content }}</p>
        </div>
    </div> <!-- end section-intro -->

    <div class="row contact-content">

        <div class="col-seven tab-full animate-this">

            <h5>{{ home.contact.actionTitle }}</h5>

            <!-- form -->
            <form name="contactForm" id="contactForm" method="post" action="{{ home.contact.form.action }}">
                <input type="text" name="_gotcha" style="display:none" />
                <div class="form-field">
                    <input name="contactName" type="text" id="contactName" placeholder="Name" value="" minlength="2"
                        required="">
                </div>

                <div class="row">
                    <div class="col-six tab-full">
                        <div class="form-field">
                            <input name="contactEmail" type="email" id="contactEmail" placeholder="Email" value=""
                                required="">
                        </div>
                    </div>
                    <div class="col-six tab-full">
                        <div class="form-field">
                            <input name="contactSubject" type="text" id="contactSubject" placeholder="Subject" value="">
                        </div>
                    </div>
                </div>

                <div class="form-field">
                    <textarea name="contactMessage" id="contactMessage" placeholder="message" rows="10" cols="50"
                        required=""></textarea>
                </div>

                <div class="form-field">
                    <button class="submitform">Submit</button>

                    <div id="submit-loader">
                        <div class="text-loader">Sending...</div>
                        <div class="s-loader">
                            <div class="bounce1"></div>
                            <div class="bounce2"></div>
                            <div class="bounce3"></div>
                        </div>
                    </div>
                </div>

            </form> <!-- end form -->

            <!-- contact-warning -->
            <div id="message-warning"></div>

            <!-- contact-success -->
            <div id="message-success">
                <i class="fa fa-check"></i>Your message was sent, thank you!<br>
            </div>

        </div> <!-- end col-seven -->

        <div class="col-four tab-full contact-info end animate-this">

            <h5>{{ home.contact.information.title }}</h5>

            <div class="cinfo">
                <h6>{{ home.contact.information.subtitle }}</h6>
                <p>
                    {{ home.contact.information.address | safe}}
                </p>
            </div> <!-- end cinfo -->

            <div class="cinfo">
                <h6>{{ home.contact.information.emailTitle }}</h6>
                <p>
                    {{ home.contact.information.emails | safe }}
                </p>
            </div> <!-- end cinfo -->

            <div class="cinfo">
                <h6>{{ home.contact.information.phoneTitle }}</h6>
                <p>
                    {{ home.contact.information.phones | safe }}
                </p>
            </div>

        </div> <!-- end cinfo -->

    </div> <!-- end row contact-content -->

</section> <!-- end contact -->


